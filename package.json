{
  "name": "bartholomew",
  "version": "2.0.0",
  "description": "Bot designed for the Prayer Room Discord server.",
  "main": "node bot.js",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "tsc && node dist/bot.js",
    "watch": "tsc-watch --noClear -p tsconfig.json --onSuccess 'node dist/bot.js'",
    "prettier": "prettier --write package.json tsconfig.json src/**/*.ts"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  },
  "author": {
    "name": "Zachary Montgomery",
    "url": "https://codingproject.org/"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "cron": "^2.0.0",
    "data-store": "^3.1.0",
    "discord.js": "^13.8.1",
    "eslint-plugin-jest": "^26.5.3",
    "moment": "^2.26.0",
    "mongodb": "^4.7.0",
    "mysql2": "^2.0.2",
    "prettier": "^2.6.2",
    "prisma": "^4.0.0",
    "rss-parser": "^3.7.2",
    "sequelize": "^6.9.0"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0"
  }
}
